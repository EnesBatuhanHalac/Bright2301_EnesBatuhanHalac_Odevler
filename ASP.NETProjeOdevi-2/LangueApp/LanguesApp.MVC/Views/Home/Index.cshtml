@model List<TeacherViewModel>

<div class="row">
  
    <div class="col-10">
        <div class="row g-3">
            @foreach (var teacher in Model)
            {
                <div class="col-2">
                   <p>@teacher.FullName</p>
                   <p>@teacher.About</p>
                   <p>@teacher.Age</p>
                   <p>@teacher.Price</p>
                   <p>@teacher.Country</p>
                   <p>@teacher.ImageUrl</p>
                </div>
            }
        </div>
    </div>
</div>